{
  "name": "html-entry-loader",
  "description": "A webpack loader of using HTML file(s) as entry",
  "version": "0.1.7",
  "author": "Mitscherlich <mitscherlich36@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mitscherlich/html-entry-loader.git"
  },
  "bugs": {
    "url": "https://github.com/Mitscherlich/html-entry-loader/issues"
  },
  "homepage": "https://github.com/Mitscherlich/html-entry-loader#readme",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "start": "npm run build -- -w --source-maps",
    "prebuild": "npm run clean",
    "build": "babel ./src -d ./lib --copy-files",
    "clean": "rimraf ./lib",
    "lint": "prettier",
    "lint:fix": "prettier --write",
    "dev": "webpack serve --config example/webpack.config.js --progress",
    "build:example": "rimraf example/dist && webpack --config example/webpack.config.js --progress",
    "build:puml": "node node_modules/.bin/puml generate flow.puml -o .",
    "prepublishOnly": "npm run build -- --no-comments"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "hash-sum": "^2.0.0",
    "html-minifier-terser": "^5.1.1",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "parse5": "^6.0.1",
    "pretty-error": "^3.0.4",
    "tapable": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.3.2",
    "puml": "^1.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.2.1",
    "url-loader": "^4.1.1",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js,*.ts": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
